# Top-most EditorConfig file
root = true

# The following are the prettier settings we use (all defaults)
# See https://prettier.io/docs/en/configuration#editorconfig

[*]
charset = utf-8
insert_final_newline = true
end_of_line = lf
indent_style = space
indent_size = 2
max_line_length = 80
